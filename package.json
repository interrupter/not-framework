{
  "name": "not-framework",
  "version": "2.4.23",
  "description": "node complimentary part for client side notFramework. + dadata.ru api",
  "scripts": {
    "test": "./node_modules/.bin/mocha --reporter spec",
    "testonly": "./node_modules/.bin/mocha --reporter spec",
    "build": "./node_modules/.bin/rollup -c",
    "production": "NODE_ENV=production ./node_modules/.bin/rollup -c",
    "lint": "./node_modules/.bin/eslint ./src/**/**.js",
    "cover": "istanbul cover _mocha $(find ./test -name \"*.js\" -not -path \"./node_modules/*\")",
    "pretest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/interrupter/not-framework.git"
  },
  "keywords": [
    "not-framework",
    "node"
  ],
  "author": "interrupter <levoe.milo@mail.ru> (http://github.com/interrupter)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/interrupter/not-node/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-polyfill": "*",
    "babel-preset-es2015-rollup": "*",
    "chai": "*",
    "chai-as-promised": "*",
    "cssnano": "*",
    "eslint": "*",
    "fs-extra": "*",
    "istanbul": "*",
    "mocha": "*",
    "postcss-cssnext": "*",
    "postcss-env-replace": "*",
    "postcss-nested": "*",
    "postcss-simple-vars": "*",
    "request": "*",
    "rollup": "*",
    "rollup-plugin-babel": "*",
    "rollup-plugin-commonjs": "*",
    "rollup-plugin-eslint": "*",
    "rollup-plugin-filesize": "*",
    "rollup-plugin-node-resolve": "*",
    "rollup-plugin-postcss": "*",
    "rollup-plugin-replace": "*",
    "rollup-plugin-sass": "*",
    "rollup-plugin-uglify": "*",
    "rollup-watch": "*"
  },
  "homepage": "https://github.com/interrupter/not-framework#readme"
}
